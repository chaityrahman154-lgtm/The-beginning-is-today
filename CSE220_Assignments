      "source": [
        "#Task 03: Decryption Process\n",
        "def col_sum(matrix,col):\n",
        "  sum=0\n",
        "  for i in range(matrix.shape[0]):\n",
        "    sum+=matrix[i][col]\n",
        "  return sum\n",
        "def decrypt_matrix(matrix):\n",
        "  arr=np.zeros(matrix.shape[1]-1,dtype=int)\n",
        "  for j in range(1,matrix.shape[1]):\n",
        "    diff=col_sum(matrix,j)-col_sum(matrix,j-1)\n",
        "    arr[j-1]=diff\n",
        "  return arr\n",
        "\n",
        "matrix=np.array([[1,3,1],\n",
        "                 [6,4,2],\n",
        "                 [5,1,7],\n",
        "                 [9,3,3],\n",
        "                 [8,5,4]])\n",
        "\n",
        "returned_array=decrypt_matrix(matrix)\n",
        "print(returned_array)\n",
        "#This should print [-13, 1]"
      ],

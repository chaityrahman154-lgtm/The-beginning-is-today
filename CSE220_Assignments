      "cell_type": "code",
      "source": [
        "def idGenerator(head1, head2, head3):\n",
        "  curr=head1\n",
        "  prev=None\n",
        "  while curr!=None:\n",
        "    temp=curr.next\n",
        "    curr.next=prev\n",
        "    prev=curr\n",
        "    curr=temp\n",
        "  temp2=head2\n",
        "  temp3=head3\n",
        "  while temp2!=None:\n",
        "    sum=temp2.elem+temp3.elem\n",
        "    if sum>=10:\n",
        "      sum=sum%10\n",
        "    temp2=temp2.next\n",
        "    temp3=temp3.next\n",
        "    new=Node(sum,None)\n",
        "    head1.next=new\n",
        "    head1=new\n",
        "  return prev\n",
        "\n",
        "print('==============Test Case 1=============')\n",
        "head1 = createList(np.array([0,3,2,2]))\n",
        "head2 = createList(np.array([5,2,2,1]))\n",
        "head3 = createList(np.array([4,3,2,1]))\n",
        "\n",
        "print(\"Linked List 1:\")\n",
        "printLinkedList(head1)\n",
        "print(\"Linked List 2:\")\n",
        "printLinkedList(head2)\n",
        "print(\"Linked List 3:\")\n",
        "printLinkedList(head3)\n",
        "\n",
        "result = idGenerator(head1, head2, head3)\n",
        "print(\"New ID:\")\n",

      "source": [
        "#Task 06: Matrix Compression\n",
        "def compress_matrix(mat):\n",
        "  new_arr=np.zeros((mat.shape[0]//2,mat.shape[1]//2),dtype=int)\n",
        "  r,c=0,0\n",
        "  for i in range(0,mat.shape[0],2):\n",
        "    for j in range(0,mat.shape[1],2):\n",
        "      sum=0\n",
        "      sum+=mat[i][j]+mat[i+1][j+1]+mat[i][j+1]+mat[i+1][j]\n",
        "      new_arr[r][c]=sum\n",
        "      c+=1\n",
        "    r+=1\n",
        "    c=0\n",
        "  return new_arr\n",
        "matrix=np.array([[1,2,3,4],\n",
        "                 [5,6,7,8],\n",
        "                 [1,3,5,2],\n",
        "                 [-2,0,6,-3]\n",
        "                 ])\n",
        "print_matrix(matrix)\n",
        "print(\"...............\")\n",
        "returned_array=compress_matrix(matrix)\n",
        "print_matrix(returned_array)\n",
        "#This should print\n",
        "\n",
        "#|  14  |  22 |\n",
        "#--------------\n",
        "#|  2  |  10  |\n",
        "#--------------"
      ],

      "cell_type": "code",
      "source": [
        "#Task 07: Game Arena\n",
        "\n",
        "def play_game(arena):\n",
        "  sum=0\n",
        "  for i in range(arena.shape[0]):\n",
        "    for j in range(arena.shape[1]):\n",
        "      if arena[i][j]!=0 and arena[i][j]%50==0:\n",
        "        if i-1>=0:\n",
        "          if arena[i-1][j]==2:\n",
        "            sum+=2\n",
        "        if j+1<arena.shape[1]:\n",
        "          if arena[i][j+1]==2:\n",
        "            sum+=2\n",
        "        if j-1>=0:\n",
        "          if arena[i][j-1]==2:\n",
        "            sum+=2\n",
        "        if i+1<arena.shape[0]:\n",
        "          if arena[i+1][j]==2:\n",
        "            sum+=2\n",
        "  if sum>=0:\n",
        "    print(f\"Points Gained: {sum}. Your team has survived the game.\")\n",
        "  else:\n",
        "    print(f\"Points Gained: {sum}. Your team is out\")\n",
        "arena=np.array([[0,2,2,0],\n",
        "                [50,1,2,0],\n",
        "                [2,2,2,0],\n",
        "                [1,100,2,0]\n",
        "                ])\n",
        "print_matrix(arena)\n",
        "play_game(arena)\n",
        "#This should print\n",
        "#Points Gained: 6. Your team is out.\n",
        "\n",
        "print(\".....................\")\n",
        "arena=np.array([[0,2,2,0,2],\n",
        "                [1,50,2,1,100],\n",
        "                [2,2,2,0,2],\n",
        "                [0,200,2,0,0]\n",
        "                ])\n",
        "print_matrix(arena)\n",
        "play_game(arena)\n",
        "#This should print\n",
        "#Points Gained: 14. Your team has survived the game."
      ],

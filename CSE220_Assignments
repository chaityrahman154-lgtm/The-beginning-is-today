        "# Task 04: Zigzag Walk\n",
        "\n",
        "def walk_zigzag(floor):\n",
        "  for j in range(floor.shape[1]):\n",
        "    if j%2==0:\n",
        "      for i in range(0,floor.shape[0],2):\n",
        "        print(floor[i][j],end=\" \")\n",
        "    else:\n",
        "      if floor.shape[0]%2==0:\n",
        "        for i in range(floor.shape[0]-1,0,-2):\n",
        "          print(floor[i][j],end=\" \")\n",
        "      else:\n",
        "        for i in range(floor.shape[0]-2,0,-2):\n",
        "          print(floor[i][j],end=\" \")\n",
        "    print()\n",
        "\n",
        "\n",
        "floor = np.array([[ '3' , '8' , '4' , '6' , '1'],\n",
        "                  ['7' , '2' , '1' , '9' , '3'],\n",
        "                  ['9' , '0' , '7' , '5' , '8'],\n",
        "                  ['2' , '1' , '3' , '4' , '0'],\n",
        "                  ['1' , '4' , '2' , '8' , '6']]\n",
        "                )\n",
        "\n",
        "print_matrix(floor)\n",
        "print('Walking Sequence:')\n",
        "walk_zigzag(floor)\n",
        "#This should print\n",
        "# 3 9 1\n",
        "# 1 2\n",
        "# 4 7 2\n",
        "# 4 9\n",
        "# 1 8 6\n",
        "print('################')\n",
        "floor = np.array([[ '3' , '8' , '4' , '6' , '1'],\n",
        "                  ['7' , '2' , '1' , '9' , '3'],\n",
        "                  ['9' , '0' , '7' , '5' , '8'],\n",
